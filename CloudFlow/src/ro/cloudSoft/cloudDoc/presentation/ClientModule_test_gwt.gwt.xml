<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN"
	"http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
	
<module rename-to="cloudDocClientModule">

	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.extjs.gxt.ui.GXT" />
  	<inherits name="com.extjs.gxt.desktop.WebDesktop" />
	
	
	<!-- >> Browserele pentru care se compileaza -->
	
	<!--
	Pentru o compilare mai rapida, se va decomenta linia corespunzatoare browser-ului pe care va fi testata aplicatia.
	ATENTIE: Daca se compileaza doar pentru un browser, pe celelalte NU va functiona corect.
	-->
	
	<!-- <set-property name="user.agent" value="gecko1_8" /> --> <!-- Compilare doar pentru Firefox -->
	<!-- <set-property name="user.agent" value="ie8" /> --> <!-- Compilare doar pentru Internet Explorer 8 -->
	
	<!-- << Browserele pentru care se compileaza -->
	
	
	<!-- >> Limba interfetei grafice -->
	
	<extend-property name="locale" values="ro" />
	<extend-property name="locale" values="en" />
	
	<!-- Pentru o compilare mai rapida, se va lasa decomentata doar linia cu o singura limba. -->
	
	<!-- <set-property name="locale" value="ro" /> --> <!-- Doar romana -->
	<!-- <set-property name="locale" value="en" /> --> <!-- Doar engleza -->
	<set-property name="locale" value="ro,en" /><!--  -->
	
	<!-- << Limba interfetei grafice -->
	
	
	<stylesheet src="css/gxt-all.css" />
    <stylesheet src="css/gxt-gray.css" />
    <stylesheet src="css/cloudDoc.css" />
    <stylesheet src="desktop/css/desktop.css" />
	
	<source path="client/shared" />
	<source path="client/client" />
	<source path="client/user" />
	
	<entry-point class="ro.cloudSoft.cloudDoc.presentation.client.client.ClientModuleEntryPoint" />
		
</module>