[th:acl] > nt:unstructured, mix:referenceable
  - th:entityId (string) mandatory
  - th:entityType (string) mandatory
  - th:permission (string) mandatory

[th:folder] > th:hierarchyNode, nt:unstructured, nt:folder
  - th:name (string) mandatory
  - th:documentTypeId (long)

[th:documentVersion] > th:document
  - th:lastStableVersion (boolean) mandatory
  - th:versionAuthor (string) mandatory
  - th:versionDescription (string) mandatory
  - th:versionWorkflowStateId (string)

[th:attachment] > nt:file, nt:base
  - th:name (string) mandatory