<jcr='http://www.jcp.org/jcr/1.0'>
<nt='http://www.jcp.org/jcr/nt/1.0'>
<mix='http://www.jcp.org/jcr/mix/1.0'>
<th='http://www.cloud-soft.eu/CloudFlow'>

[th:hierarchyNode] > nt:base, mix:referenceable
  orderable
  - th:description (string)
  - th:author (string) mandatory
  - th:lockedBy (string)
  - th:created (date) mandatory
  - th:lastModified (date) mandatory
  - th:acl (string) multiple

[th:documentLocation] > mix:referenceable, nt:unstructured
  - th:name (string) mandatory
  - th:description (string)

[th:documentLocationSecurity] > nt:base
  - th:entitiesWithAccess (string) multiple

